{
	"name": "GitHub compare helper",
	"version": "1.5.0",
	"description": "Adds UI to GitHub commits pages to easier create GitHub compare urls",
	"author": "Henrik Gunnarsson",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"page_action" :
	{
		"default_icon" : "icon-19.png"
	},
	"permissions" : [
		"declarativeContent",
		"tabs",
		"https://github.com/*"
	],
	"icons" : {
		"19" : "icon-19.png",
		"48" : "icon-48.png",
		"128" : "icon-128.png"
	},
	"manifest_version": 2
}
